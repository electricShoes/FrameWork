<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grid id='grdList' desc='학습데이터 마스터'>
    <meta>
        
        <readonly       value="N" />             <!-- 읽기 전용여부 -->
        <showCheckbox   value="N" />             <!-- 좌측 체크박스 표시여부 -->
        <showNumber     value="Y" />             <!-- 좌측 번호컬럼 표시여부 -->
        <rowSelection   value="N" />             <!-- 클릭시 전체 로우 선택여부 -->
        <insertable     value="Y" />             <!-- 로우 삽입가능 여부 -->
        <appendable     value="Y" />             <!-- 로우 추가가능 여부 -->
        <editable       value="Y" />             <!-- 로우 추가가능 여부 -->
        <deletable      value="Y" />             <!-- 로우 삭제 가능 여부 -->
        <rollbackable   value="N" />             <!-- 로우 복구 가능 여부 -->
        <gridfitStyle   value="evenFill" />
        <columnMove     value="N" />             <!-- 그리드 컬럼이동 -->
        <showHeaderSum  value="N" />             <!-- 헤더영역 합계표시 여부 -->
        <showFooter     value="N" />             <!-- footer영역 여부 -->
        <setSort        value="exclusive" />     <!-- 그리드 정렬 방식  기본값:none,  exclusive: 클릭한 컬럼, incusive: 클릭순으로 멀티 , revers: 마지막 클릭순으로 멀티    -->
        <showStateBar   value="Y" />             <!-- 그리드 상태표시 여부 기본값:Y -->
       
        
        <!--    defaultValue="Y"    -->
        <!--    required="Y"    -->
        <!--    maxLength="10"    -->
        <!--    width='120'    -->
        <!--    Pk = 'Y'    -->
    </meta>
    <!-- textAlignment : left:기본(좌측정렬), center:중앙정렬, right:우측정렬  -->
    <column field='stdyDtaMngNo'    name='학습데이터관리번호'    type='char'  width='80' mstCd ="STDY_DTA_MNG_NO"   textAlignment='left' hidden="Y" />
    <column field='dtasNm'    name='촬영기기'    type='combo'  width='120'  mstCd ="DTAS_CD"   textAlignment='center' readOnly="Y" />
    <column field='lattDd'    name='위도'      type='char'  width='80' mstCd ="LATT_DD"   textAlignment='right' readOnly="Y" />
    <column field='lngtDd'    name='경도'      type='char' width='80' mstCd ="LNGT_DD" textAlignment='right' readOnly="Y" />
    <column field='seaNm'    name='해역'      type='combo' width='120' mstCd ="PORT_CD" textAlignment='center' readOnly="Y" />
    <column field='dpthOfWter'    name='수심'      type='char' width='80' mstCd ="DPTH_OF_WTER" textAlignment='right' readOnly="Y" />
    <column field='rmk'    name='비고'      type='char' width='120' mstCd ="RMK" textAlignment='left'  readOnly="Y" />
    <column field='imgBtn'    name='이미지'     type='button' width='30' mstCd ="IMG_BTN" textAlignment='center'/>
</grid>